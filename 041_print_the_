/* print the following pattern using `for` loop:
    *
    **
    ***
    ****
    *****
*/






#include<stdio.h>
int main()
{
	int num;
	printf("enter no of lines in the pattern\n");
	scanf("%d",&num);
	for(int i=0;i<num;i++)
	{
		for(int j=0;j<(i+1);j++)
		{
			printf("*");
		}
		printf("\n");
	} 
}
